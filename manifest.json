{
    "manifest_version": 3,
    "name": "Dolar exchance",
    "version": "1.0",
    "description": "Extensión para convertir dólares a bolívares venezolanos.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["https://pydolarve.org/api/v1/*"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Consulta de tasas",
        "default_popup": "popup/popup.html"
    }
}
